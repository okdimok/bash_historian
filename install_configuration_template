#!/usr/bin/env bash
DIR="$( cd "$( dirname "${BASH_SOURCE[0]}" )" >/dev/null 2>&1 && pwd )"

BASH_HISTORY_TEMPLATE_EDITED= # just set it to something
BASH_HISTORY_REPOSITORY=
