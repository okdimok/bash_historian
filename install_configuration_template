BASH_HISTORY_SSH_KEY=

INSTALL_GITCONFIG= #just set it to anything to install gitconig
GIT_USER_NAME=
GIT_USER_EMAIL=

TELEGRAM_INSTALLATION_KEYS=
SLACK_HOOK_ENDPOINT_CONFIG="https://hooks.slack.com/services/"